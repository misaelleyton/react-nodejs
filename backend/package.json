{
  "name": "TS-Node-Docker-starter",
  "version": "1.0.0",
  "description": "Starter code for node ts app running in a docker container with hot reload",
  "scripts": {
    "build": "tsc",
    "start:dev": "node -r dotenv/config dist/server.js",
    "start:watch": "nodemon -r dotenv/config src/server.ts",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Kartik K",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "body-parser": "^1.19.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "http-errors": "^1.8.0",
    "jest": "^29.4.2",
    "joi": "^17.3.0",
    "mongoose": "^5.11.6",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.3.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "express": "^4.17.2"
  }
}
